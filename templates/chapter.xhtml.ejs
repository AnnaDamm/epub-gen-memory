<%- docHeader %>
<head>
  <meta charset="UTF-8" />
  <title><%= title %></title>
  <link rel="stylesheet" type="text/css" href="style.css" />
</head>
<body>
  <% if (title && appendChapterTitles) { %>
    <h1><%= title %></h1>
  <% } %>
  <% if (title && author.length) { %>
    <p class="epub-author"><%= author.join(', ') %></p>
  <% } %>
  <% if (title && url) { %>
    <p class="epub-link"><a href="<%= url %>"><%= url %></a></p>
  <% } %>
  <%- content %>
</body>
</html>